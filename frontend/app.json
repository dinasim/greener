{
  "expo": {
    "name": "Greener",
    "slug": "greener",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "originalFullName": "@dinasim/greener",
    "owner": "dina2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "greener",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dinasim.greener",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app requires access to your microphone for voice search.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourname.greener",
      "intentFilters": [
    {
      "action": "VIEW",
      "data": {
        "scheme": "greener",
        "host": "",
        "pathPrefix": "/"
      },
      "category": ["BROWSABLE", "DEFAULT"]}],
      "permissions": [
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "expoClientId": "241318918547-d2qq18eqm12h44f9eshvpifr1vql7lpr.apps.googleusercontent.com",
      "webClientId": "241318918547-d2qq18eqm12h44f9eshvpifr1vql7lpr.apps.googleusercontent.com",
      "iosClientId": "241318918547-cmj5p79839ci9kdrgjmbuskpfu6j4e6h.apps.googleusercontent.com",
      "androidClientId": "241318918547-tctcf9f11ii3ndn0ilohgqbjpjbej9a8.apps.googleusercontent.com",
      "eas": {
        "projectId": "6915a87a-3492-4315-b77e-ca7cfbf609fc"
      }
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
